<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_translator">

    <fragment
        android:id="@+id/navigation_translator"
        android:name="com.example.prototypea.ui.translator.TranslatorFragment"
        android:label="@string/title_translator"
        tools:layout="@layout/translator" />

    <fragment
        android:id="@+id/navigation_lesson"
        android:name="com.example.prototypea.ui.lesson.LessonFragment"
        android:label="@string/title_lesson"
        tools:layout="@layout/lesson" />

    <fragment
        android:id="@+id/navigation_game"
        android:name="com.example.prototypea.ui.game.GameFragment"
        android:label="@string/title_game"
        tools:layout="@layout/game" />
</navigation>